{
  "manifest_version": 2,
  "name": "Sololearn Forge",
  "version": "1.0",
  "description": "Sololearn Forge",
  "icons": {
    "48": "/img/icon.png"
  },
  "browser_action": {
    "default_icon": "/img/icon.png",
    "default_popup": "/html/popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/js/jquery-3.4.1.min.js",
        "/js/axios.min.js",
        "/js/content.js"
      ]
    }
  ]
}